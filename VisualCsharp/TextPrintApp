namespace WinFormsApp4
{
    public partial class Form1 : Form
    {
        public Form1()
        {
            InitializeComponent();
        }

        private void button1_Click(object sender, EventArgs e)
        {
            textBox2.Text = textBox1.Text.Substring(0,3); //3개를 가져온다. 0~3까지
            textBox3.Text = textBox1.Text.Substring(textBox1.TextLength-3 , 3); //전체길이에 -3 한후 뒤에서 세자리 가져옴
            textBox4.Text = textBox1.TextLength.ToString(); //길이
            textBox5.Text = textBox1.TextLength.ToString();
        }
    }
}
